#include <stdio.h>
int main(void){
   int num;
   printf("Enter the marks: ");
   scanf("%d",&num);
   printf(" You entered %d", num); 

	    if(80<=num<=100){
	      printf(" You got A grade"); 
		}
	    else if (70<= num <=84){ 
		   printf(" You got B grade");
		}
	    else if (55<= num<= 69 ){
		   printf(" You got C grade");
		}
    	else if (40<= num <= 54){
	    	printf(" You got D grade");
		}
        else if ( num <40){
	    	printf(" You are FAIL");
        }
return 0;
}
